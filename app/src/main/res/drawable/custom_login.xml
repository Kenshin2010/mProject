<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>

            <solid android:color="#d9ec0c" />
            <corners
                android:radius="10dp">
            </corners>
            <stroke
                android:color="@color/custom"
                android:width="3dp">
            </stroke>
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp">
            </padding>
        </shape>
    </item>

    <item >
        <shape>

            <solid android:color="#11ed7e00" />
            <corners
                android:radius="10dp">
            </corners>
            <stroke
                android:color="@color/custom"
                android:width="3dp">
            </stroke>
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp">
            </padding>
        </shape>
    </item>

</selector>